    "@mui/material": "^7.3.1",
    "@reduxjs/toolkit": "^2.8.2",
    "axios": "^1.11.0",
    "i18next": "^25.3.2",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19",
    "react-i18next": "^15.6.1",
    "react-redux": "^9.2.0",
    "react-router": "^6 || ^7",
    "single-spa": "^6.0.2",
    "single-spa-react": "^6.0.1",
    "tslib": "^2.8.1",
    "vite-plugin-react-single-spa-hmr": "^1.0.0"
